{
    "manifest_version": 2,
    "name": "ReTwitterify",
    "version": "1.2",
    "description": "Turns X back to Twitter.",
    "author": "Xenoreaper",
    "icons": {
        "48": "icons/ReTwitterify.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.twitter.com/*",
                "*://*.x.com/*"
            ],
            "js": [
                "retwitterify.js"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html"
    }
}